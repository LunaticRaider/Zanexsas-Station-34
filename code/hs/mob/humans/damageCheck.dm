/mob/living/carbon/human/proc/damageCheck()
	var/damage = rand(1,6)
	switch(src.alternian_blood_type)
		if("Mutant")
			damage *= rand(0.1,0.5) //base human damage
		if("Rust")
			damage *= rand(2,20)
		if("Bronze")
			damage *= rand(10,30)
		if("Gold")
			damage *= rand(10,30)
		if("Lime")
			damage *= rand(40,80)
		if("Jade")
			damage *= rand(40,80)
		if("Olive")
			damage *= rand(40,80)
		if("Teal")
			damage *= rand(80,120)
		if("Cerulean")
			damage *= rand(80,120)
		if("Indigo")
			damage *= rand(80,1000)
		if("Violet")
			damage *= rand(300,600)
		if("Purple")
			damage *= rand(360,700)
		if("Fuchsia")
			damage *= rand(450,1000)
	damage = abs(damage)
	return damage
